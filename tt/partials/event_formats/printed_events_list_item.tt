[%#
--------------------------------------------------------------------------------
tt/partials/event_formats/printed_events_list_item.tt
--------------------------------------------------------------------------------
%]

<tr valign="top">
    <td>
        [%~ event.dates %]
[%- UNLESS event.times == 'TBC' ~%]
        <br>[% event.times %]
[%- END ~%]
    </td>
    <td>[% event.name FILTER html %]</td>
    <td>
[%- IF ( matches = event.presented_by.match ( '^"(.+)"$' ) ) -%]
        [% matches.0 %]<br>
[%- ELSE ~%]
        Presented by [% event.presented_by %]<br>
[%- END ~%]
        [% '(' _ event.box_office _ ')' ~%]
    </td>
    <td>
        [%~ event.venue_name ~%]
    </td>
</tr>
